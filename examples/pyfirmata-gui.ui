    <property name="type_hint">dialog</property>
    <property name="program_name">pyFirmate GUI</property>
    <property name="version">0.1</property>
    <property name="copyright">Copyright (C) 2012, Fabian Affolter &lt;fabian@affolter-engineering.ch&gt;</property>
