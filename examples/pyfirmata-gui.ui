    <property name="website">https://bitbucket.org/tino/pyfirmata/</property>
    <property name="website_label" translatable="yes">pyFirmata</property>
    <property name="authors">Fabian Affolter &lt;fabian@affolter-engineering.ch&gt;</property>
    <property name="license_type">bsd</property>
